@model List<TableViewModel>

<h1>MASA LİSTESİ</h1>

<a class="btn btn-primary" asp-area="Executive" asp-controller="Table" asp-action="Add">Masa Ekle</a>

<h5 style="color:red">@ViewBag.TableEror</h5>
<table class="table table-stripped">
    <tr>
        <th>TableNo</th>
        <th></th>

    </tr>
    @foreach(var item in Model)
    {
      <tr>
          <td>@item.TableNo</td>
          <td>
              <a asp-area="Executive" asp-controller="Table" asp-action="Update" asp-route-id="@item.Id" class="btn btn-success">Güncelle</a>
              <a asp-area="Executive" asp-controller="Table" asp-action="Delete" asp-route-id="@item.Id" class="btn btn-danger">-</a>
          </td>
      </tr>
    }
</table>